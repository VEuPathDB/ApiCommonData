<wdkModel>

  <!-- notes

   - eventually projectId and Project param should be combined


  -->

    <paramSet name="recordParams">
    
      <stringParam name="primaryKey" prompt="Primary Key"/>

      <stringParam name="projectId" prompt="Project ID"/>

      <stringParam name="source_id" prompt="Primary Key"/>

      <stringParam name="project_id" prompt="Project ID"/>

      <stringParam name="Query" prompt="Project ID"/>

      <enumParam name="deprecated_genes" prompt="Deprecated Genes">
       <enumList>
         <enumValue>
            <term>deprecated_genes</term>
            <internal>1</internal>
          </enumValue>
          <enumValue>
            <term>genes</term>
            <internal>0</internal>
          </enumValue>
      </enumList>
    </enumParam>


      <enumParam name="assemblage_genes" prompt="Assemblage Genes">
       <enumList>
         <enumValue>
            <term>Assemblage A isolate WGS</term>
            <internal>Giardia lamblia ATCC 50803</internal>
          </enumValue>
          <enumValue>
            <term>Assemblage B isolate GS</term>
            <internal>Giardia intestinalis ATCC 50581</internal>
          </enumValue>
          <enumValue>
            <term>Assemblage E isolate P15</term>
            <internal>Giardia lamblia P15</internal>
          </enumValue>
      </enumList>
    </enumParam>


      <enumParam name="Project" prompt="Project">
        <useTermOnly value="true"/>
       <enumList>
         <enumValue>
            <term>CryptoDB</term>
            <internal>CryptoDB</internal>
          </enumValue>
          <enumValue>
            <term>PlasmoDB</term>
            <internal>PlasmoDB</internal>
          </enumValue>
          <enumValue>
            <term>ToxoDB</term>
            <internal>ToxoDB</internal>
          </enumValue>
          <enumValue>
                <term>GiardiaDB</term>
                <internal>GiardiaDB</internal>
          </enumValue>
          <enumValue>
                <term>TrichDB</term>
                <internal>TrichDB</internal>
          </enumValue>
          <enumValue>
                <term>TriTrypDB</term>
                <internal>TriTrypDB</internal>
          </enumValue>
        </enumList>
      </enumParam>

    <answerParam name="gene_answer"
                 recordClassRef="GeneRecordClasses.GeneRecordClass"
                 prompt="Gene Answer"
                 readonly="true" />

    <answerParam name="genomic_answer"
                 recordClassRef="SequenceRecordClasses.SequenceRecordClass"
                 prompt="Genomic Sequence Answer"
                 readonly="true" />

   <answerParam name="isolate_answer"
                 recordClassRef="IsolateRecordClasses.IsolateRecordClass"
                 prompt="Isolate Answer"
                 readonly="true" includeProjects="EuPathDB,PlasmoDB,ToxoDB,CryptoDB"/>

    <answerParam name="snps_answer"
                 recordClassRef="SnpRecordClasses.SnpRecordClass"
                 prompt="SNP Answer"
                 readonly="true" includeProjects="EuPathDB,PlasmoDB,ToxoDB,CryptoDB"/>

    <answerParam name="est_answer"
                 recordClassRef="EstRecordClasses.EstRecordClass"
                 prompt="EST Answer"
                 readonly="true"/>

    <answerParam name="assembly_answer"
                 recordClassRef= "AssemblyRecordClasses.AssemblyRecordClass"
                 prompt="Assembly Answer"
                 readonly="true" excludeProjects="TrichDB"/>

    <answerParam name="orf_answer"
                 recordClassRef= "OrfRecordClasses.OrfRecordClass"
                 prompt="ORF Answer"
                 readonly="true"/>

    <answerParam name="sagetag_answer"
                 recordClassRef= "SageTagRecordClasses.SageTagRecordClass"
                 prompt="SAGE Tags Answer"
                 readonly="true" includeProjects="EuPathDB,GiardiaDB,ToxoDB,PlasmoDB"/>
 

