<wdkModel>

<!-- notes

 -these are the recordclasses used for the Ajax Question pages

-->

  <recordClassSet name="AjaxRecordClasses">

<!--=====================================================================-->
<!-- INTERPRO TERM CLASSES-->
<!--=====================================================================-->

        <!-- Interpro Term class -->
        <recordClass name="InterproTermClass" doNotTest="true" type="Ontology">
            <!-- primary key definition -->
            <primaryKeyAttribute name="primary_key"
                                 displayName="Gene">
                <columnRef>source_id</columnRef>
                <columnRef>project_id</columnRef>
                <text>
                    <![CDATA[
                    $$project_id$$:$$source_id$$
                    ]]>
                </text>                
            </primaryKeyAttribute>

            <table name="InterproTerms" displayName="Pfam Terms"
                   queryRef="AjaxTables.InterproTermList">
                <columnAttribute name="accession"/>
                <columnAttribute name="name"/>
                <columnAttribute name="description"/>
            </table>
        </recordClass> 

