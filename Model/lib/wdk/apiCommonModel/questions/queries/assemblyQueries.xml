    <!-- EST Source id  -->
    <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->

    <sqlQuery name="ByEstAccession" doNotTest="true" includeProjects="CryptoDB,ToxoDB,GiardiaDB,PlasmoDB,TriTrypDB"
         isCacheable="true">
        <paramRef ref="estParams.est_id"/>
        <column name="source_id"/>
        <column name="project_id"/>
        <sql>
          <![CDATA[
             SELECT distinct aa.source_id, aa.project_id
             FROM apidb.EstAttributes ea, apidb.AssemblyAttributes aa,  @WDK_ENGINE_SCHEMA@dataset_values@USER_DBLINK@ ds
             WHERE ds.dataset_id = $$est_id$$
              AND LOWER(ea.source_id) LIKE LOWER(REPLACE(REPLACE(ds.dataset_value,' ',''), '*', '%'))
               AND ea.assembly_source_id = aa.source_id
               AND aa.project_id = '@PROJECT_ID@'
          ]]>
       </sql>
    </sqlQuery>

 <processQuery name="ByEstAccession" includeProjects="EuPathDB"
              isCacheable="true" processName="org.apidb.apicomplexa.wsfplugin.apifed.ApiFedPlugin">
        <paramRef ref="estParams.est_id"/>
        <paramRef ref="sharedParams.signature" quote="false"/>
        <wsColumn name="source_id" width="50" wsName="source_id"/>
        <wsColumn name="project_id" width="32" wsName="project_id"/>
